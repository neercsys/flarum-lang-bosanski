{
    "name": "neercsys/flarum-lang-bosanski",
    "type": "flarum-extension",
    "description": "Bosnian language pack.",
    "homepage": "https://github.com/neercsys/flarum-lang-bosanski",
    "keywords": ["locale","translation","bosanski","bosnian"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Bosanski",
            "category": "language",
            "info": {
                "website": "https://diskusije.balkanbuzz.net",
                "support": "https://diskusije.balkanbuzz.net"
            },
            "icon": {
                "image": "icon.png",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "bs",
            "title": "Bosanski"
        }
    }
